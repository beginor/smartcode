@using SmartCode
@using SmartCode.Generator.Entity
@using static SmartCode.Generator.RazorTemplates.Contributions.AppFx.Common
@inherits SmartCode.TemplateEngine.Impl.RazorCoreTemplate<SmartCode.BuildContext>
@{
    var project = Model.Project;
    var buildTask = Model.Build;
    var table = Model.GetCurrentTable();
}
using System;
using System.Linq;
using System.Threading.Tasks;
using NHibernate;
using NHibernate.NetCore;
using NUnit.Framework;
using @(project.Author).@(project.Module).Data.Entities;
using @(project.Author).@(project.Module).Data.Repositories;
using @(project.Author).@(project.Module).Models;

namespace @(project.Author).@(project.Module).@(buildTask.Module).Data;

/// <summary>@table.GetSummary()仓储测试</summary>
[TestFixture]
public class @(table.ConvertedName)RepositoryTest : BaseTest<I@(table.ConvertedName)Repository> {

    [Test]
    public void _01_CanResolveTarget() {
        Assert.IsNotNull(Target);
    }

    [Test]
    public async Task _02_CanDoSearchAsync() {
        var searchModel = new @(table.ConvertedName)SearchModel {
            Skip = 0,
            Take = 10
        };
        var result = await Target.SearchAsync(searchModel);
        Assert.GreaterOrEqual(result.Total, 0);
        Assert.GreaterOrEqual(result.Take, result.Data.Count);
    }

}
